{
   "name": "ml-controller-api",
   "version": "1.0.0",
   "description": "Mercado Livre controller API",
   "main": "app.ts",
   "repository": "https://github.com/dvsouto/ml-controller-api",
   "author": "Davi Souto <davi.souto@gmail.com>",
   "license": "UNLICENSED",
   "private": true,
   "scripts": {
      "start": "ts-node src/index.ts",
      "dev": "ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpile-only --ignore-watch node_modules --no-notify --poll src/app.ts",
      "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
      "test": "jest",
      "docker-create": "docker compose up",
      "docker-start": "docker compose up -d",
      "docker-stop": "docker compose down",
      "docker-remove": "docker compose down -v --rmi all",
      "log-app": "docker logs --follow ml-controller-api-app",
      "log-postgres": "docker logs --follow ml-controller-api-postgres",
      "typeorm": "ts-node --require ts-node/register ./node_modules/typeorm/cli.js",
      "migrate:create": "./scripts/migrate-create.sh",
      "migrate:up": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --no-notify --poll ./scripts/migrate-up.ts",
      "migrate:down": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules --no-notify --poll ./scripts/migrate-down.ts"
   },
   "bin": {
      "typeorm": "./cli.js",
      "typeorm-ts-node-commonjs": "./cli-ts-node-commonjs.js",
      "typeorm-ts-node-esm": "./cli-ts-node-esm.js"
   },
   "dependencies": {
      "axios": "^0.27.2",
      "dotenv": "^16.0.1",
      "express": "^4.18.1",
      "lodash": "^4.17.21",
      "moment": "^2.29.4",
      "node-localstorage": "^2.2.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "ts-node": "10.7.0",
      "typeorm": "0.3.6",
      "yarn": "^1.22.19"
   },
   "devDependencies": {
      "@babel/cli": "^7.18.6",
      "@babel/core": "^7.18.6",
      "@babel/node": "^7.18.6",
      "@babel/plugin-transform-arrow-functions": "^7.18.6",
      "@babel/preset-env": "^7.18.6",
      "@babel/preset-typescript": "^7.18.6",
      "@types/express": "^4.17.13",
      "@types/jest": "^28.1.3",
      "@types/lodash": "^4.14.182",
      "@types/node": "^16.11.10",
      "@typescript-eslint/eslint-plugin": "^5.30.0",
      "@typescript-eslint/parser": "^5.30.0",
      "babel-plugin-module-resolver": "^4.1.0",
      "eslint": "^8.18.0",
      "jest": "^28.1.1",
      "ts-jest": "^28.0.5",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^4.0.0",
      "typescript": "4.5.2"
   }
}
